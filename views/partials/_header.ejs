<header>TinyApp</header>
Welcome <%=username%><br>Login successful!<br> 

<% if(username){ %>
  <form method="POST" action="/logout">
  <button>Logout</button>
  </form>
 <% } else{ %>  
  <form method="POST" action="/login">
  <label for="username">Username:</label>
  <input name="username" style="width: 30%; font-size: 15px">
  <button>Login</button>
  </form>
<% } %>




